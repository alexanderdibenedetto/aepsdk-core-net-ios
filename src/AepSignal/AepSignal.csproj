<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0-ios</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>true</ImplicitUsings>
    <IsBindingProject>true</IsBindingProject>
  </PropertyGroup>

  <ItemGroup>
    <ObjcBindingApiDefinition Include="ApiDefinition.cs" />
    <ObjcBindingCoreSource Include="StructsAndEnums.cs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\AepCore\AepCore.csproj" />
    <ProjectReference Include="..\AepServices\AepServices.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="References\.DS_Store" />
    <None Remove="References\AEPSignal.framework\AEPSignal" />
    <None Remove="References\AEPSignal.framework\Info.plist" />
    <None Remove="References\AEPSignal.framework\Headers\AEPSignal-Swift.h" />
    <None Remove="References\AEPSignal.framework\Headers\AEPSignal.h" />
    <None Remove="References\AEPSignal.framework\Modules\module.modulemap" />
    <None Remove="References\AEPSignal.framework\Modules\AEPSignal.swiftmodule\arm64-apple-ios.swiftinterface" />
    <None Remove="References\AEPSignal.framework\Modules\AEPSignal.swiftmodule\arm64-apple-ios.swiftdoc" />
    <None Remove="References\AEPSignal.framework\Modules\AEPSignal.swiftmodule\arm64-apple-ios.abi.json" />
    <None Remove="References\AEPSignal.framework\Modules\AEPSignal.swiftmodule\arm64-apple-ios.private.swiftinterface" />
  </ItemGroup>
  <ItemGroup>
    <NativeReference Include="References\AEPSignal.framework">
      <Kind>Framework</Kind>
      <SmartLink>False</SmartLink>
      <LinkerFlags>-Objc</LinkerFlags>
    </NativeReference>
  </ItemGroup>
</Project>
